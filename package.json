  "bugs": {
    "url": "https://github.com/HoseungJang/flickable-scroll/issues"
  },
  "homepage": "https://github.com/HoseungJang/flickable-scroll#readme",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "rollup": "^3.26.2",
    "rollup-plugin-dts": "^5.3.0",
    "typescript": "^5.1.6"
  }
