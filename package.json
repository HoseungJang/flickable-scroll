{
  "name": "flickable-scroll",
  "version": "0.0.2",
  "description": "A flickable web scroller",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "esm"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
